#include <stdio.h>
#define n 5

int main() {
    
    int a[n] = {2, 1, 3, 5, 4};
    int i, j, temp;

    for (i = 0; i < (n - 1); i++) {
        int flag = 0;
        for (j = 0; j < (n - 1 - i); j++) {
            if (a[j] > a[j + 1]) {
                temp = a[j];
                a[j] = a[j + 1];
                a[j + 1] = temp;
                flag = 1;
            }
        }
        if (flag == 0) {
            break;
        }
    }

     
// Print the sorted array
    for (i = 0; i < n; i++) {
        printf("%d ", a[i]);
    }
    printf("\n");

    return 0;
}
