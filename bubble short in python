n = 5

a = [2, 1, 3, 5, 4]

for i in range(n - 1):
    flag = 0
    for j in range(n - 1 - i):
        if a[j] > a[j + 1]:
            temp = a[j]
            a[j] = a[j + 1]
            a[j + 1] = temp
            flag = 1
    if flag == 0:
        break

# Print the sorted array
for i in range(n):
    print(a[i], end=" ")
print()
